from collections import deque

goal = "123456780"   # Goal state (0 = blank)

# Moves: up, down, left, right
moves = {0:[1,3],1:[0,2,4],2:[1,5],3:[0,4,6],4:[1,3,5,7],5:[2,4,8],6:[3,7],7:[4,6,8],8:[5,7]}

def solve(start):
    start = start.replace(" ","")  # remove spaces
    q = deque([(start, "")])
    seen = {start}
    
    while q:
        state, path = q.popleft()
        if state == goal:
            return path
        zero = state.index("0")
        for m in moves[zero]:
            s = list(state)
            s[zero], s[m] = s[m], s[zero]
            new = "".join(s)
            if new not in seen:
                seen.add(new)
                q.append((new, path+new+" "))
    return None

# Example start state (0 = blank)
start = "125340678"
ans = solve(start)

if ans:
    print("Solved!")
else:
    print("No solution.")
